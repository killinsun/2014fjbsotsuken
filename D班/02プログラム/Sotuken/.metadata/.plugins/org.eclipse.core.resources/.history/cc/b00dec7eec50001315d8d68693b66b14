      out.write("    <link rel=\"stylesheet\" type=\"text/css\" href=\"user.css\" />\r\n");
      out.write("</head>\r\n");
      out.write("<body>\r\n");
      out.write("<div class=\"frm\">\r\n");
      out.write("\t<!--header部分-->\r\n");
      out.write("\t<header id=\"header\">\r\n");
      out.write("\t\t\t<div id=\"title\">\r\n");
      out.write("\t\t\tユーザー登録画面\r\n");
      out.write("\t\t</div>\r\n");
      out.write("\t\t<div id=\"account\" align=\"right\">\r\n");
      out.write("\t\t\t<div id=\"name\">苗字　名前</div>\r\n");
      out.write("\t\t\t<!--ログアウトボタン-->\r\n");
      out.write("\t\t\t<button type=\"button\" id=\"logout\">ログアウト</button>\r\n");
      out.write("\t\t</div>\r\n");
      out.write("\t\t<div>\r\n");
      out.write("\t\tこの部分に新着メールの情報が表示されます。 \r\n");
      out.write("\t\t</div>\r\n");
      out.write("\t\r\n");
      out.write("\t</header>\r\n");
      out.write("\t\r\n");
      out.write("\t<!--ページ移動、ナビゲーション部分-->\r\n");
      out.write("\t<nav id=\"nav\">\r\n");
      out.write("\t\t\r\n");
      out.write("\t\t\t<iframe src=\"./../mainMenu.jsp\" name=\"mainMenu\" Align=\"left\" Frameborder=\"no\" Marginheight=\"0\" Width=\"200\">\r\n");
      out.write("\r\n");
      out.write("\t\t\t</iframe>\r\n");
      out.write("\t\t\r\n");
      out.write("\t</nav>\r\n");
      out.write("\t\r\n");
      out.write("\t<!--作業画面-->\r\n");
      out.write("\t<section id=\"mainContent\">\r\n");
      out.write("\t\t ユーザーID：<br />\r\n");
      out.write("\t\t <input type=\"text\" id=\"id\" name=\"id\" placeholder=\"半角英数\"/><br />\r\n");
      out.write("\t\t パスワード：<br />\r\n");
      out.write("\t\t <input type=\"password\" id=\"password\" name=\"password\" placeholder=\"半角英数\"/><br />\r\n");
      out.write("\t\t 氏名：<br />\r\n");
      out.write("\t\t <input type=\"text\" id=\"name\" name=\"name\" placeholder=\"例：船橋　太郎\"/><br />\r\n");
      out.write("\t\t フリガナ：<br />\r\n");
      out.write("\t\t <input type=\"text\" id=\"name\" name=\"name\" placeholder=\"例：フナバシ　タロウ\"/><br />\r\n");
      out.write("\t\t 性別：<br />\r\n");
      out.write("\t\t\t<select name=\"性別\">\r\n");
      out.write("    \t\t\t<option>男</option>\r\n");
      out.write("    \t\t\t<option>女</option>\r\n");
      out.write("  \t\t\t</select><br />\r\n");
      out.write("\t\t 生年月日：<br />\r\n");
      out.write("\t\t <input type=\"text\" name=\"birthday\" id=\"birthday\" placeholder=\"例：1991/01/01\"/><br />\r\n");
      out.write("\t\t 住所：<br />\r\n");
      out.write("\t\t <input type=\"text\" id=\"address\" name=\"address\" placeholder=\"例：千葉県柏市本町1-1-1\"/><br />\r\n");
      out.write("\t\t 電話番号：<br />\r\n");
